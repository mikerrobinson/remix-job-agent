{
  "name": "remix-job-agent",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "contentful-codegen": "ts-node contentful-typescript-codegen --output @types/generated/contentful.d.ts",
    "cf-typegen": "wrangler types",
    "deploy": "npm run build && wrangler deploy",
    "dev": "react-router dev",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "postinstall": "npm run cf-typegen",
    "preview": "npm run build && vite preview",
    "typecheck": "npm run cf-typegen && react-router typegen && tsc -b"
  },
  "dependencies": {
    "@contentful/rich-text-html-renderer": "^17.1.3",
    "@react-router/fs-routes": "^7.9.1",
    "contentful": "^11.7.19",
    "isbot": "^5.1.27",
    "lucide-react": "^0.544.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.7.1"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.0.12",
    "@eslint/js": "^9.36.0",
    "@react-router/dev": "^7.7.1",
    "@tailwindcss/typography": "^0.5.18",
    "@tailwindcss/vite": "^4.1.4",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "contentful-ts-generator": "^0.2.12",
    "eslint": "^9.36.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.4.0",
    "htmlhint": "^1.7.1",
    "tailwindcss": "^4.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.37.0"
  }
}
